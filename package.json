    "classnames": "^2.2.6",
    "gatsby": "^2.15.15",
    "gatsby-image": "^2.2.19",
    "gatsby-mdx": "^1.0.0",
    "gatsby-plugin-feed": "^2.3.12",
    "gatsby-plugin-force-trailing-slashes": "^1.0.4",
    "gatsby-plugin-manifest": "^2.2.16",
    "gatsby-plugin-mdx": "^1.0.41",
    "gatsby-plugin-offline": "^3.0.6",
